# Docusaurus Environment Configuration
# Copy this file to .env.local for local development
# Values marked with [required] must be set for production

# Site Configuration
SITE_URL=https://gasnet.org
BASE_URL=/

# Giscus Comments Configuration (GitHub Discussions-based comments)
# Configure Giscus after the repo is created. repoId/categoryId are provided by Giscus.
# See https://giscus.app for the configuration wizard.
GISCUS_REPO=your-org/your-repo
GISCUS_REPO_ID=your-repo-id
GISCUS_CATEGORY=General
GISCUS_CATEGORY_ID=your-category-id

# Privacy-Preserving Analytics (Umami)
# Self-hosted, GDPR/CCPA compliant analytics
ANALYTICS_ENABLED=false
ANALYTICS_WEBSITE_ID=
ANALYTICS_HOST=https://analytics.gasnet.org

# Deployment
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=

# Build Configuration
NODE_ENV=production
